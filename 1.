def two(nums,target):
    seen = {}
    for i, num in enumerate(nums):
         lol = target - num
         if lol in seen:
              return[seen[lol],i]
         seen[num] = i
print(two([2, 7, 11, 15], 9))  # Output: [0, 1]
print(two([3, 2, 4], 6))       # Output: [1, 2]
print(two([3, 3], 6))          # Output: [0, 1]
